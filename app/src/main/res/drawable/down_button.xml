<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <rotate android:fromDegrees="180.0" android:toDegrees="180.0"
            android:pivotX="50%" android:pivotY="50%" android:drawable="@drawable/up_b_pressed" />
    </item>
    <item android:state_focused="true">
        <rotate android:fromDegrees="180.0" android:toDegrees="180.0"
            android:pivotX="50%" android:pivotY="50%" android:drawable="@drawable/up_b_focus" />
    </item>
    <item >
        <rotate android:fromDegrees="180.0" android:toDegrees="180.0"
            android:pivotX="50%" android:pivotY="50%" android:drawable="@drawable/up_b"/>
    </item>
</selector>